body,ul,li,a{
	margin: 0 auto;
	padding: 0;
	background-size: 100% 100%;
}
ul,li {
    list-style: none;
}       
a{
    text-decoration: none;
    color: black;
}
.wrap{
	width:100%;
}
#header {
	width: 100%;
	height:120px;
	background-color: rgb(96,96,96);
}
#logo {
	display: inline-block;
}
#logo img {
	margin-top:0;
	margin-left:200px;
}
#sign {
	display: inline-block;
	position:relative;
	left: 50%;
	bottom: 10px;
}
#sign #leaf {
	display: inline-block;
	width: 29px;
	height: 33px;
	background-image: url(../images/leaf.png);
	background-repeat: no-repeat;
	vertical-align:bottom;
}
#sign a {
	font-size: 29px;
	height: 33px;
	line-height: 33px;
	vertical-align: bottom;
	color: rgb(239,206,134);
	text-decoration: none;
}
#sign a:hover {
	color: rgb(220,220,220);
}
/*头部结束*/
.main{
	width: 100%;
	height:520px;
	margin: auto;
	position: relative;
	background-image: url(../images/background.jpg);
	background-size: 100% 100%;
}
@media screen and (min-width: 1380px){
	.main{
		width: 100%;
		height: 700px;
	}
}
@media screen and (min-width: 1920px){
	.main{
		width: 100%;
		height:825px;
	}
}

