<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_1494933819964">
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>setCharFilter</filter-name>
    <filter-class>com.dywz.common.utils.setCharFilter</filter-class>
    <init-param>
      <param-name>encode</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>setCharFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>userFilter</filter-name>
    <filter-class>com.dywz.common.utils.userFilter</filter-class>
  	<init-param>
      <param-name>not</param-name>
      <param-value>login.jsp,UserServlet,css,js,png,jpg,eot,svg,ttf,woff,gif</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>userFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>